{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#525252",
          "foreground": "#ffffff",
          "type": "time",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .CurrentDate | date .Format }} "
        },
        {
          "background": "#8800dd",
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true,
            "style": "austin",
            "threshold": 500
          },
          "style": "powerline",
          "template": " {{ .FormattedMs }} ",
          "powerline_symbol": "\ue0b0",
          "type": "executiontime"
        },
        {
          "background": "#ff0000",
          "background_templates": [
            "{{ if eq .Code 0 }}#6ca35e{{ end }}"
          ],
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true,
            "status_template": "{{ if eq .Code 0 }}\u2713{{ else }}\u2717{{ end }} {{ .Code }}"
          },
          "style": "powerline",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#546e7a",
          "background_templates": [
            "{{ if eq .OS \"windows\" }}#025da3{{ end }}",
            "{{ if eq .OS \"ubuntu\" }}#dd4814{{ end }}",
            "{{ if eq .OS \"raspbian\" }}#c31c4a{{ end }}",
            "{{ if eq .OS \"linux\" }}#da1717{{ end }}"
          ],
          "foreground": "#eeeeee",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}  ",
          "type": "os"
        },
        {
          "background": "#546e7a",
          "foreground": "#eeeeee",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}{{ if .SSHSession }}@{{ .HostName }}{{ end }} ",
          "type": "session"
        },
        {
          "background": "#ffff66",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#7a7c0b",
          "background_templates": [
            "{{ if and (eq .OS \"windows\") (eq .Shell \"bash\") }}#a14242{{ end }}",
            "{{ if eq .Shell \"pwsh\" }}#477cda{{ end }}",
            "{{ if eq .Shell \"powershell\" }}#ca1e1e{{ end }}",
            "{{ if eq .Shell \"bash\" }}#64686d{{ end }}",
            "{{ if eq .Shell \"zsh\" }}#008a2e{{ end }}",
            "{{ if eq .Shell \"cmd\" }}#e224e2{{ end }}"
          ],
          "foreground": "#ffffff",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "templates_logic": "first_match",
          "templates": [
            "{{ if and (eq .OS \"windows\") (eq .Shell \"bash\") }} git bash {{ end }}",
            " {{ .Name }} "
          ],
          "type": "shell"
        },
        {
          "background": "#48d3d8",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf115",
            "style": "full"
          },
          "style": "powerline",
          "templates_logic": "first_match",
          "templates": [
            " {{ if and (eq .OS \"windows\") (eq .Shell \"bash\") }}/{{ lower (substr 0 1 .Location) }}{{ (substr 2 10000 .Location) | replace \"\\\\\" \"/\" }}{{ end }} ",
            " {{ .Path }} "
          ],
          "type": "path"
        },
        {
          "background": "#3dff6e",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f3ff48{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#fa5e3b{{ end }}",
            "{{ if gt .Ahead 0 }}#ad89dc{{ end }}",
            "{{ if gt .Behind 0 }}#89d1dc{{ end }}"
          ],
          "foreground": "#193549",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}\uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#1976d2",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue753 {{ .Full }} ",
          "type": "angular"
        },
        {
          "background": "#9ec3f0",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ufd03 {{ .EnvironmentName }}",
          "type": "az"
        },
        {
          "background": "#06a4ce",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue798 {{ .Full }} ",
          "type": "dart"
        },
        {
          "background": "#06a4ce",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue28e {{ .Full }} ",
          "type": "flutter"
        },
        {
          "background": "#00ffff",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue77f {{ .Full }} ",
          "type": "dotnet"
        },
        {
          "background": "#7fd5ea",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ufcd1 {{ .Full }} ",
          "type": "go"
        },
        {
          "background": "#4063d8",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "properties": {
            "cache_version": true
          },
          "template": " \ue738 {{ .Full }} ",
          "type": "java"
        },
        {
          "background": "#a7cae1",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "properties": {
            "display_mode": "files"
          },
          "template": " Helm {{ .Version }} ",
          "type": "helm"
        },
        {
          "background": "#6ca35e",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#906cff",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ .Full }} ",
          "type": "python"
        },
        {
          "background": "#99908a",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue7a8 {{ .Full }} ",
          "type": "rust"
        },
        {
          "background": "transparent",
          "foreground": "transparent",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "type": "text",
          "template": "&nbsp;",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "upgrade",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#193549",
          "background": "#db2b2b",
          "template": " \udb80\uddda {{ .Current }} => {{ .Latest }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "white",
          "style": "plain",
          "template": " \uf101 ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}}ADMIN: {{end}}{{.Folder}} ({{.Shell}})",
  "final_space": true,
  "version": 2
}
