{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#546e7a",
          "background_templates": [
            "{{ if eq .OS \"windows\" }}#025da3{{ end }}",
            "{{ if eq .OS \"ubuntu\" }}#dd4814{{ end }}",
            "{{ if eq .OS \"raspbian\" }}#c31c4a{{ end }}",
            "{{ if eq .OS \"linux\" }}#da1717{{ end }}"
          ],
          "foreground": "#eeeeee",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#546e7a",
          "foreground": "#eeeeee",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#ffff66",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#7a7c0b",
          "background_templates": [
            "{{ if eq .Shell \"pwsh\" }}#477cda{{ end }}",
            "{{ if eq .Shell \"powershell\" }}#ca1e1e{{ end }}",
            "{{ if eq .Shell \"bash\" }}#64686d{{ end }}",
            "{{ if eq .Shell \"zsh\" }}#008a2e{{ end }}",
            "{{ if eq .Shell \"cmd\" }}#e224e2{{ end }}"
          ],
          "foreground": "#ffffff",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "style": "diamond",
          "template": " {{ .Name }} ",
          "trailing_diamond": "\ue0b0",
          "type": "shell"
        },
        {
          "background": "#1976d2",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue753 {{ .Full }} ",
          "type": "angular"
        },
        {
          "background": "#9ec3f0",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ufd03 {{ .EnvironmentName }}",
          "type": "az"
        },
        {
          "background": "#06a4ce",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue798 {{ .Full }} ",
          "type": "dart"
        },
        {
          "background": "#06a4ce",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue28e {{ .Full }} ",
          "type": "flutter"
        },
        {
          "background": "#00ffff",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue77f {{ .Full }} ",
          "type": "dotnet"
        },
        {
          "background": "#7fd5ea",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ufcd1 {{ .Full }} ",
          "type": "go"
        },
        {
          "background": "#4063d8",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue738 {{ .Full }}",
          "type": "java"
        },
        {
          "background": "#ebcc34",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": "{{if .Namespace}} \ufd31 {{.Context}} :: {{.User}} :: {{.Cluster}} {{.Namespace}}{{end}} ",
          "type": "kubectl"
        },
        {
          "background": "#6ca35e",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#906cff",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ .Full }} ",
          "type": "python"
        },
        {
          "background": "#99908a",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue7a8 {{ .Full }} ",
          "type": "rust"
        },
        {
          "background": "#48d3d8",
          "foreground": "#000000",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "properties": {
            "folder_icon": "\uf115",
            "style": "full"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#3dff6e",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f3ff48{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#fa5e3b{{ end }}",
            "{{ if gt .Ahead 0 }}#f17c37{{ end }}",
            "{{ if gt .Behind 0 }}#89d1dc{{ end }}"
          ],
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "powerline",
          "template": " {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}\uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#8800dd",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "properties": {
            "always_enabled": true,
            "style": "austin",
            "threshold": 500
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#6ca35e",
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ if gt .Code 0 }}\u2717  {{ .Code }}{{ else }}\u2713{{ end }} ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "white",
          "style": "plain",
          "template": " \uf101",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}}ADMIN: {{end}}{{.Folder}} ({{.Shell}})",
  "final_space": true,
  "version": 2
}
