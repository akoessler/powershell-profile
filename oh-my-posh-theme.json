{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}}ADMIN: {{end}}{{.Folder}} ({{.Shell}})",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": []
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "",
          "foreground": "#EEEEEE",
          "background": "#546E7A",
          "background_templates": [
            "{{ if eq .OS \"windows\" }}#025da3{{ end }}",
            "{{ if eq .OS \"linux\" }}#DD4814{{ end }}",
            "{{ if eq .OS \"ubuntu\" }}#DD4814{{ end }}",
            "{{ if eq .OS \"raspbian\" }}#C31C4A{{ end }}"
          ],
          "properties": {
            "wsl": "",
            "wsl_separator": "",
            "postfix": "  "
          }
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#008a2e",
          "background_templates": [
            "{{ if eq .Shell \"pwsh\" }}#477CDA{{ end }}",
            "{{ if eq .Shell \"bash\" }}#293036{{ end }}"
          ],
          "properties": {
            "mapped_shell_names": {
            }
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#EEEEEE",
          "background": "#546E7A",
          "properties": {
            "display_host": false
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#111111",
          "background": "#ffff66",
          "properties": {
            "root_icon": "\uF0E7"
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "<transparent, #48d3d8>\uE0B0</>",
          "trailing_diamond": "\uE0B0",
          "foreground": "#000000",
          "background": "#48d3d8",
          "properties": {
            "style": "full",
            "folder_icon": "\uF115"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#193549",
          "background": "#3dff6e",
          "properties": {
            "display_stash_count": true,
            "display_upstream_icon": true,
            "status_colors_enabled": true,
            "display_status": true,
            "local_changes_color": "#f3ff48",
            "ahead_and_behind_color": "#fa5e3b",
            "behind_color": "#f17c37",
            "ahead_color": "#89d1dc",
            "stash_count_icon": "\uF692 "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments":
      [
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "\uE0B2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "properties": {
            "prefix": " ",
            "color_background": true,
            "error_color": "red",
            "success_icon": "\u2713",
            "error_icon": "\u2717 ",
            "always_numeric": true,
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\uE0B2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#8800dd",
          "properties": {
            "always_enabled": true,
            "threshold": 500,
            "style": "austin",
            "prefix": " "
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\uE0B4",
          "invert_powerline": true,
          "background": "#33658A",
          "foreground": "#ffffff",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "white",
          "properties": {
            "prefix": "",
            "text": " \uf101",
            "postfix": ""
          }
        }
      ]
    }
  ],
  "final_space": true
}
