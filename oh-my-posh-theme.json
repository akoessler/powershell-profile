{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}}ADMIN: {{end}}{{.Folder}} ({{.Shell}})",
  "final_space": true,
  "version": 1,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": []
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "",
          "foreground": "#eeeeee",
          "background": "#546e7a",
          "background_templates": [
            "{{ if eq .OS \"windows\" }}#025da3{{ end }}",
            "{{ if eq .OS \"ubuntu\" }}#dd4814{{ end }}",
            "{{ if eq .OS \"raspbian\" }}#c31c4a{{ end }}",
            "{{ if eq .OS \"linux\" }}#da1717{{ end }}"
          ],
          "properties": {
            "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#eeeeee",
          "background": "#546e7a",
          "properties": {
            "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }} "
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#111111",
          "background": "#ffff66",
          "properties": {
            "template": " \uf0e7 "
          }
        },
        {
          "type": "shell",
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#7a7c0b",
          "background_templates": [
            "{{ if eq .Shell \"pwsh\" }}#477cda{{ end }}",
            "{{ if eq .Shell \"bash\" }}#64686d{{ end }}",
            "{{ if eq .Shell \"zsh\" }}#008a2e{{ end }}",
            "{{ if eq .Shell \"cmd\" }}#e224e2{{ end }}"
          ],
          "properties": {
            "template": " {{ .Name }} "
          }
        },
        {
          "type": "angular",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#000000",
          "background": "#1976d2",
          "properties": {
            "template": " \ue753 {{ .Full }} "
          }
        },
        {
          "type": "az",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#000000",
          "background": "#9ec3f0",
          "properties": {
            "template": " \ufd03 {{ .EnvironmentName }}"
          }
        },
        {
          "type": "dart",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#06a4ce",
          "properties": {
            "template": " \ue798 {{ .Full }} "
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#000000",
          "background": "#00ffff",
          "properties": {
            "template": " \ue77f {{ .Full }} "
          }
        },
        {
          "type": "go",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#7fd5ea",
          "properties": {
            "template": " \ufcd1 {{ .Full }} "
          }
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#4063d8",
          "properties": {
            "template": " \ue738 {{ .Full }}"
          }
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#000000",
          "background": "#ebcc34",
          "properties": {
            "template": " \ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} "
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#6ca35e",
          "properties": {
            "template": " \ue718 {{ .Full }} "
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#100e23",
          "background": "#906cff",
          "properties": {
            "template": " \ue235 {{ .Full }} "
          }
        },
        {
          "type": "rust",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#193549",
          "background": "#99908a",
          "properties": {
            "template": " \ue7a8 {{ .Full }} "
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "foreground": "#000000",
          "background": "#48d3d8",
          "properties": {
            "style": "full",
            "folder_icon": "\uf115",
            "template": " {{ .Path }} "
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#193549",
          "background": "#3dff6e",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f3ff48{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#fa5e3b{{ end }}",
            "{{ if gt .Ahead 0 }}#f17c37{{ end }}",
            "{{ if gt .Behind 0 }}#89d1dc{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_worktree_count" : true,
            "fetch_upstream_icon": true,
            "template": " {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}\uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} "
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "",
          "foreground": "#ffffff",
          "background": "#8800dd",
          "properties": {
            "always_enabled": true,
            "threshold": 500,
            "style": "austin",
            "template": " {{ .FormattedMs }} "
          }
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#6ca35e",
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": " {{ if gt .Code 0 }}\u2717  {{ .Code }}{{ else }}\u2713{{ end }} "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "white",
          "properties": {
            "template": " \uf101"
          }
        }
      ]
    }
  ]
}
